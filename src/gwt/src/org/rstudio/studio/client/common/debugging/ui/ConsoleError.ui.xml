<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	.consoleErrorBox {
	   border: 1px dashed #c0c0c0;
	   position: relative;
	   overflow: hidden;
	   padding: 5px;
	   margin: 5px 5px 5px 0px;
	}

	.consoleErrorTraceback {
	   right: 5px;
	   top: 4px;
	   position: absolute;
	}
	</ui:style>
	<g:HTMLPanel styleName="{style.consoleErrorBox}">
	  <g:Label ui:field="errorMessage"></g:Label>
	  <g:Button styleName="{style.consoleErrorTraceback}" ui:field="showTraceback">
	     Show Traceback
	  </g:Button>
	  <g:HTMLPanel ui:field="framePanel" visible="false">
	  </g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder> 