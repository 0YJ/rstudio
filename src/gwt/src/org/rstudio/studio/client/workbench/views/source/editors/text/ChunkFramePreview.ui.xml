<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
xmlns:g="urn:import:com.google.gwt.user.client.ui">
<ui:style>

.header, .type, .data
{
  height: 2px;
  border-radius: 3px;
  background-color: black;
  right: 0px;
  top: 0px;
}

.type 
{
  opacity: 0.25;
  height: 2px;
}

.data
{
  height: 1px;
}

.separator
{
  opacity: 0.25;
  width: 100%;
  height: 1px;
  background-color: black;
}

.cell
{
  width: 33%;
}
</ui:style>
<g:HTML width="100%" height="100%">
<table width="100%" height="100%">
   <tr>
      <th class="cell"><div class="{style.header}" style="width: 15px"></div></th>
      <th class="cell"><div class="{style.header}" style="width: 18px"></div></th>
      <th class="cell"><div class="{style.header}" style="width: 22px"></div></th>
   </tr>
   <tr>
      <td class="cell"><div class="{style.type}" style="width: 15px"></div></td>
      <td class="cell"><div class="{style.type}" style="width: 10px"></div></td>
      <td class="cell"><div class="{style.type}" style="width: 15px"></div></td>
   </tr>
   <tr>
      <td colspan="3"><div class="{style.separator}"></div></td>
   </tr>
   <tr>
      <td class="cell"><div class="{style.data}" style="width: 10px"></div></td>
      <td class="cell"><div class="{style.data}" style="width: 11px"></div></td>
      <td class="cell"><div class="{style.data}" style="width: 13px"></div></td>
   </tr>
   <tr>
      <td class="cell"><div class="{style.data}" style="width: 13px"></div></td>
      <td class="cell"><div class="{style.data}" style="width: 10px"></div></td>
      <td class="cell"><div class="{style.data}" style="width: 11px"></div></td>
   </tr>
   <tr>
      <td class="cell"><div class="{style.data}" style="width: 13px"></div></td>
      <td class="cell"><div class="{style.data}" style="width: 10px"></div></td>
      <td class="cell"><div class="{style.data}" style="width: 11px"></div></td>
   </tr>
   <tr>
      <td class="cell"><div class="{style.data}" style="width: 18px"></div></td>
      <td class="cell"><div class="{style.data}" style="width: 10px"></div></td>
      <td class="cell"><div class="{style.data}" style="width: 12px"></div></td>
   </tr>
   <tr>
      <td class="cell"><div class="{style.data}" style="width: 10px"></div></td>
      <td class="cell"><div class="{style.data}" style="width: 11px"></div></td>
      <td class="cell"><div class="{style.data}" style="width: 13px"></div></td>
   </tr>
   <tr>
      <td class="cell"><div class="{style.data}" style="width: 10px"></div></td>
      <td class="cell"><div class="{style.data}" style="width: 11px"></div></td>
      <td class="cell"><div class="{style.data}" style="width: 13px"></div></td>
   </tr>
   <tr>
      <td class="cell"><div class="{style.data}" style="width: 18px"></div></td>
      <td class="cell"><div class="{style.data}" style="width: 10px"></div></td>
      <td class="cell"><div class="{style.data}" style="width: 12px"></div></td>
   </tr>
   <tr>
      <td class="cell"><div class="{style.data}" style="width: 18px"></div></td>
      <td class="cell"><div class="{style.data}" style="width: 10px"></div></td>
      <td class="cell"><div class="{style.data}" style="width: 12px"></div></td>
   </tr>
   <tr>
      <td class="cell"><div class="{style.data}" style="width: 18px"></div></td>
      <td class="cell"><div class="{style.data}" style="width: 10px"></div></td>
      <td class="cell"><div class="{style.data}" style="width: 12px"></div></td>
   </tr>
</table>
</g:HTML>
</ui:UiBinder>