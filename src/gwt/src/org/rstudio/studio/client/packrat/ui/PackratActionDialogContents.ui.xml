<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:rw="urn:import:org.rstudio.core.client.widget"
	xmlns:rc="urn:import:org.rstudio.studio.client.common" xmlns:rmd="urn:import:org.rstudio.studio.client.rmarkdown.ui"
	xmlns:c="urn:import:com.google.gwt.user.cellview.client" xmlns:rs="urn:import:org.rstudio.core.client.cellview">


	<ui:style>
	@eval proportionalFont org.rstudio.core.client.theme.ThemeFonts.getProportionalFont();
	.RestoreActionsTable {
		height: 300px;
		width: 600px;
	}
	
	.RestoreActionsTable th {
		white-space: nowrap;
		overflow: hidden;
		text-overflow: ellipsis;
		padding-left: 4px;
		font-family: proportionalFont;
		border-bottom: 1px solid #d6dadc;
		border-right: 1px solid #d6dadc;
		border-top: 1px solid white;
		border-left: 1px solid white;
		padding: 1px 2px;
		color: black;
		background-color: #eeeff1;
		font-weight: normal;
		text-align: center;
		border-collapse: separate;
		font-size: 11px;
	}
	</ui:style>

	<g:HTMLPanel>
		<c:DataGrid ui:field="table_" styleName="{style.RestoreActionsTable}" />
	</g:HTMLPanel>

</ui:UiBinder> 
