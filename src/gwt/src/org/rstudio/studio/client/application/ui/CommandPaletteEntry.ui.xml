<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style field="styles_" type="org.rstudio.studio.client.application.ui.CommandPaletteEntry.Styles">
		.entry {
			width: 100%;
			font-size: 10pt;
			margin-bottom: 5px;
		}
		
		.selected {
			background-color: #A0A0FF;
		}
	</ui:style>
	<g:HorizontalPanel styleName="{styles_.entry}">
		<g:cell horizontalAlignment="ALIGN_LEFT">
			<g:Label ui:field="name_"></g:Label>
		</g:cell>
		<g:cell horizontalAlignment="ALIGN_RIGHT">
			<g:Label ui:field="shortcut_"></g:Label>
		</g:cell>
	</g:HorizontalPanel>
</ui:UiBinder> 